{
  "extends": [
    "./node_modules/gts/",
    "plugin:react/recommended"
  ],
  "overrides": [
    {
      "files": ["**/*.js", "**/*.ts", "**/*.tsx"],
      "rules": {
        "node/no-extraneous-import": "off",
        "node/no-extraneous-require": "off",
        "node/no-unpublished-import": "off"
      }
    }
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
